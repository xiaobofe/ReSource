{
    "name": "ReSource",
    "version": "1.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "ReSource",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "options_page": "options.html",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "unlimitedStorage"
    ]
}